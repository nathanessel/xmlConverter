package converter;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;

public class ProcessRevision {
	String assemblyName = "";
	
	Map<String, ProcessRevisionDataElement> processRevisionDataElement = new HashMap<String, ProcessRevisionDataElement>();
	Map<String, ProcessRevisionDataElement> operationElement = new HashMap<String, ProcessRevisionDataElement>();
	Map<String, ProcessRevisionDataElement> stepElement = new HashMap<String, ProcessRevisionDataElement>();
	Map<String, ProcessRevisionDataElement> documentElement = new HashMap<String, ProcessRevisionDataElement>();
	Map<String, ProcessRevisionDataElement> activityElement = new HashMap<String, ProcessRevisionDataElement>();
	Map<String, ProcessRevisionDataElement> exitPathwayElement = new HashMap<String, ProcessRevisionDataElement>();
	
	List<Map<String, ProcessRevisionDataElement>> operationsElement = new ArrayList<Map<String, ProcessRevisionDataElement>>();
	List<Map<String, ProcessRevisionDataElement>> stepsElement = new ArrayList<Map<String, ProcessRevisionDataElement>>();
	List<Map<String, ProcessRevisionDataElement>> documentsElement = new ArrayList<Map<String, ProcessRevisionDataElement>>();
	List<Map<String, ProcessRevisionDataElement>> activitiesElement = new ArrayList<Map<String, ProcessRevisionDataElement>>();
	List<Map<String, ProcessRevisionDataElement>> exitPathwaysElement = new ArrayList<Map<String, ProcessRevisionDataElement>>();
	
	public ProcessRevision(String assemblyName) {
		this.assemblyName = assemblyName;
	}
	
	public void clearProcessRevisionDataElements() {
		processRevisionDataElement = new HashMap<String, ProcessRevisionDataElement>();
		operationElement = new HashMap<String, ProcessRevisionDataElement>();
		stepElement = new HashMap<String, ProcessRevisionDataElement>();
		documentElement = new HashMap<String, ProcessRevisionDataElement>();
		activityElement = new HashMap<String, ProcessRevisionDataElement>();
		exitPathwayElement = new HashMap<String, ProcessRevisionDataElement>();
	}

}
